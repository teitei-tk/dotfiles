{
  "editor.cursorStyle": "block",
  "workbench.statusBar.visible": true,
  "workbench.sideBar.location": "left",
  "editor.fontFamily": "Hack, Menlo, Monaco, 'Courier New', monospace",
  "editor.fontSize": 15,
  "workbench.iconTheme": "file-icons",
  "ruby.format": "rubocop",
  "ruby.lint": {
    "rubocop": true,
    "rails": true
  },
  "vim.statusBarColorControl": true,
  "vim.statusBarColors.normal": ["#8ac6f2", "#444444"],
  "vim.statusBarColors.insert": ["#95e454", "#444444"],
  "vim.statusBarColors.visual": ["#f2c68a", "#444444"],
  "vim.statusBarColors.visualline": ["#f2c68a", "#444444"],
  "vim.statusBarColors.visualblock": ["#f2c68a", "#444444"],
  "vim.statusBarColors.replace": ["#e5786d", "#444444"],
  "workbench.colorTheme": "GitHub Dark Dimmed",
  "workbench.colorCustomizations": {
    "statusBar.background": "#8ac6f2",
    "statusBar.noFolderBackground": "#8ac6f2",
    "statusBar.debuggingBackground": "#8ac6f2",
    "statusBar.foreground": "#444444",
    "statusBar.debuggingForeground": "#444444"
  },
  "activitusbar.activeColour": "#999",
  "activitusbar.inactiveColour": "#444",
  "tslint.autoFixOnSave": true,
  "tslint.packageManager": "yarn",
  "tslint.run": "onType",
  "workbench.startupEditor": "newUntitledFile",
  "editor.find.globalFindClipboard": true,
  "vim.useSystemClipboard": true,
  "editor.minimap.enabled": false,
  "editor.formatOnSave": true,
  "[javascript]": {
    "editor.formatOnSave": true
  },
  "[ruby]": {
    "editor.formatOnSave": false
  },
  "files.trimFinalNewlines": true,
  "files.trimTrailingWhitespace": true,
  "editor.tabSize": 2,
  "editor.formatOnSaveTimeout": 2000,
  "vim.mouseSelectionGoesIntoVisualMode": true,
  "breadcrumbs.enabled": true,
  "eslint.alwaysShowStatus": true,
  "eslint.run": "onSave",
  "eslint.validate": ["javascript", "javascriptreact"],
  "window.zoomLevel": 0,
  "explorer.autoReveal": false,
  "ruby.rubocop.useBundler": true,
  "ruby.useBundler": true,
  "window.clickThroughInactive": false,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit"
  },
  "javascript.updateImportsOnFileMove.enabled": "always",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "markdown.preview.breaks": true,
  "launch": {
    "version": "0.2.0",
    "configurations": [
      {
        "name": "Debug Jest Tests",
        "type": "node",
        "request": "launch",
        "runtimeArgs": [
          "--inspect-brk",
          "${workspaceRoot}/node_modules/.bin/jest",
          "--runInBand"
        ],
        "console": "integratedTerminal",
        "internalConsoleOptions": "neverOpen",
        "port": 9229
      },
      {
        "name": "Go Debug and Tests",
        "type": "go",
        "request": "launch",
        "mode": "auto",
        "program": "${fileDirname}"
      }
    ]
  },
  "go.formatTool": "goimports",
  "[html]": {
    "editor.formatOnSave": false
  },
  "go.useLanguageServer": true,
  "vim.normalModeKeyBindings": [
    {
      "before": ["<C-n>"],
      "after": ["g", "b"]
    }
  ],
  "vim.visualModeKeyBindings": [
    {
      "before": ["<C-n>"],
      "after": ["g", "b"]
    }
  ],
  "gopls": {
    "build.expandWorkspaceToModule": true
  },
  "go.toolsEnvVars": { "GO111MODULE": "on" },
  "solargraph.autoformat": true,
  "solargraph.checkGemVersion": false,
  "solargraph.diagnostics": true,
  "go.toolsManagement.autoUpdate": true,
  "eslint.format.enable": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "[go]": {
    "editor.insertSpaces": false,
    "editor.formatOnSave": true,
    "editor.codeActionsOnSave": {
      "source.organizeImports": "explicit"
    },
    "editor.defaultFormatter": "golang.go",
    "editor.suggest.snippetsPreventQuickSuggestions": false
  },
  "csharp.suppressDotnetInstallWarning": true,
  "security.workspace.trust.startupPrompt": "never",
  "security.workspace.trust.enabled": false,
  "redhat.telemetry.enabled": false,
  "dart.flutterHotReloadOnSave": "always",
  "[dart]": {
    "editor.defaultFormatter": "Dart-Code.dart-code",
    "editor.formatOnSave": true
  },
  "editor.inlineSuggest.enabled": true,
  "[python]": {
    "editor.formatOnType": true
  },
  "[markdown]": {
    "editor.formatOnSave": false
  },
  "liveshare.guestApprovalRequired": true,
  "terminal.integrated.defaultProfile.osx": "zsh"
}
