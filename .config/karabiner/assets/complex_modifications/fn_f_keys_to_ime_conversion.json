        {
          "type": "basic",
          "from": {
            "key_code": "f6",
            "modifiers": {
              "mandatory": ["fn"]
            }
          },
          "to": [
            {
              "key_code": "japanese_hiragana"
            }
          ]
        }
      ]
    },
    {
      "description": "fn+F7 to Katakana",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "f7",
            "modifiers": {
              "mandatory": ["fn"]
            }
          },
          "to": [
            {
              "key_code": "japanese_katakana"
            }
          ]
        }
      ]
    },
    {
      "description": "fn+F8 to Half-width Katakana",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "f8",
            "modifiers": {
              "mandatory": ["fn"]
            }
